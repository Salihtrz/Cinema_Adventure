@model EntityLayer.Class.Cast

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div>
    <dl class="row">
        <div class="col-sm-12">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.CastName)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.CastName)
            </dd>
        </div>
        <div class="col-sm-12">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.CastSurname)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.CastSurname)
            </dd>
        </div>
        <div class="col-sm-12">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.CastPicture)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.CastPicture)
            </dd>
        </div>
    </dl>
